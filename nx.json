{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "nxCloudId": "697d4f4f8298878e61cf1dcf",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "eslint:lint"
      }
    }
  ],
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"]
    },
    "lint": {
      "cache": true
    },
    "test": {
      "cache": true
    }
  },
  "generators": {
    "@nx/js:library": {
      "buildable": true,
      "publishable": false,
      "importPath": "@scratch/{name}"
    }
  }
}
